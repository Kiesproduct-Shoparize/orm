{
    "name": "benzine/benzine-database",
    "description": "The Benzine Database Access Layer. Includes generation of models, controllers and so on from schema.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Matthew Baggett",
            "email": "matthew@baggett.me"
        }
    ],
    "require": {
        "php": ">=7.4",
        "benzine/benzine-configuration": "dev-master",
        "benzine/benzine-traits": "dev-master",
        "laminas/laminas-db": "^2.11.1",
        "mattketmo/camel": "^1.1",
        "cheprasov/php-cli-args": "^3.0",
        "twig/twig": "^2.11",
        "gone.io/twig-extension-transform": "^1.0",
        "gone.io/twig-extension-inflection": "^1.0"
    },
    "require-dev": {
        "benzine/benzine-style": "dev-master",
        "benzine/benzine-test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "⌬\\Database\\": "src",
            "⌬\\Database\\Tests\\": "tests/"
        }
    },
    "bin": [
        "src/Generator/zenderator",
        "src/Generator/sdkifier",
        "src/Generator/wait-for-db"
    ]
}
